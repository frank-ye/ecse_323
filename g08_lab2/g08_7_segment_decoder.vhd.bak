library lpm;
use lpm.lpm_components.all;
library ieee;
use ieee.std_logic_1164.all;


entity g08_7_segment_decoder is

	port ( code: in std_logic_vector(3 downto 0);
		    mode: in std_logic;
		    segments_out: out std_logic_vector(6 downto 0));		 
		 
end g08_7_segment_decoder;